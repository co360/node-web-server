{
     "name": "@mike-jung/ubiaccess-server",
     "version": "1.0.0",
     "description": "Ubiaccess Server",
     "publishConfig": { "registry": "https://npm.pkg.github.com/" },
     "main": "index.js",
     "scripts": {
          "test": "echo \"Error: no test specified\" && exit 1",
          "copy": "copyfiles --soft --up 1 public/**/*.* ./dist/public",
          "build": "rimraf dist/ && babel ./ --out-dir dist/ --ignore ./node_modules,./.babelrc,./package.json,./package-lock.json,./npm-debug.log,./public --copy-files",
          "start": "node index.js",
          "rm": "rimraf dist/ ",
          "build-norm": "babel ./ --out-dir dist/ --ignore ./dist,./node_modules,./.babelrc,./package.json,./package-lock.json,./npm-debug.log,./public --copy-files",
          "start-norm": "npm run build-norm && npm run copy && node dist/index.js"
     },
     "keywords": [],
     "author": "Mike",
     "license": "ISC",
     "dependencies": {
          "@conga/annotations": "^1.0.3",
          "StringScanner": "0.0.3",
          "axios": "^0.19.0",
          "base64-js": "^1.3.1",
          "bunyan": "^1.8.12",
          "bunyan-rotating-file-stream": "^1.6.3",
          "canvas": "^2.6.0",
          "conga-annotations": "^1.0.0",
          "connect-flash": "^0.1.1",
          "cookie-parser": "^1.4.3",
          "cors": "^2.8.5",
          "ejs": "^2.7.1",
          "express": "^4.17.1",
          "express-session": "^1.16.2",
          "hashmap": "^2.4.0",
          "http": "0.0.0",
          "ioredis": "^4.14.1",
          "multer": "^1.4.2",
          "mysql": "^2.17.1",
          "mysql-utilities": "^1.1.0-alpha.3",
          "network": "^0.4.1",
          "node-uuid": "^1.4.8",
          "passport": "^0.4.0",
          "passport-local": "^1.0.0",
          "qr-image": "^3.2.0",
          "socket.io": "^2.3.0",
          "socket.io-redis": "^5.2.0",
          "winston": "^2.4.4",
          "winston-daily-rotate-file": "^1.7.2"
     },
     "devDependencies": {
          "babel-cli": "^6.26.0",
          "babel-preset-es2015": "^6.24.1",
          "copyfiles": "^2.1.1",
          "electron": "^7.1.3",
          "eslint": ">=4.18.2",
